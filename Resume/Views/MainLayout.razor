@inherits LayoutComponentBase

<ErrorBoundary>
    <ChildContent>
        <MudLayout>
            @Body
        </MudLayout>
    </ChildContent>
    <ErrorContent>
        <LayoutView Layout="@typeof(MainLayout)">
            <p role="alert">@context.Message</p>
        </LayoutView>
    </ErrorContent>
</ErrorBoundary>
